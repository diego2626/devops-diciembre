pipeline {
    agent any

    stages {
        stage('SCM') {
            steps {
                git branch: 'master', url:'https://github.com/diego2626/devops-diciembre.git'
            }
        }
        
        stage('Build') {
            steps {
               bat(script: 'echo "hola" ', returnStdout:true);
            }
        }
        
        stage('Push') {
            steps {
               bat(script: 'dir' , returnStdout:true);
            }
        }
    }
}